{
    "family": "tipstaff-task",
    "taskRoleArn": "arn:aws:iam::913862848426:role/task-tipstaff",
    "executionRoleArn": "arn:aws:iam::913862848426:role/execution-tipstaff",
    "containerDefinitions": [
      {
        "name": "tipstaff-container",
        "image": "913862848426.dkr.ecr.eu-west-2.amazonaws.com/tipstaff-ecr-repo",
        "essential": true,
        "memory": 1024,
        "cpu": 512,
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 0,
            "protocol": "tcp"
          }
        ]
      }
    ],
    "requiresCompatibilities": [
      "EC2"
    ],
    "networkMode": "bridge",
    "cpu": "512",
    "memory": "1024"
}